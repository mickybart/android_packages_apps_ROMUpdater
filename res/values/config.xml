<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- ROM Update Config Strings -->
    <string name="conf_update_server_url_def" translatable="false">https://localhost/api</string>
    <string name="conf_changelog_url" translatable="false">http://localhost/changelog.xml</string>

    <!-- Can be enable if the conf_request_with_method support incremental download -->
    <bool name="conf_support_incremental">false</bool>

    <!-- Permit to define which method to use to get the json file
         cmrest : CyanogenMod Rest API
         static : Direct file download
    -->
    <string name="conf_request_with_method" translatable="false">static</string>

    <!-- Set to true if your recovery used OpenRecoveryScript (like TWRP) -->
    <bool name="conf_use_openrecoveryscript">true</bool>
</resources>
